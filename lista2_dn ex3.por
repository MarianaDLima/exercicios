programa {
  funcao inicio() {
//3. Faça um Programa que peça as 4 notas bimestrais e mostre a média.
  real not1,not2,not3,not4
  escreva("Digite a primeira nota:\n")
   leia(not1)
   escreva("Digite a segunda nota: \n")
   leia(not2)
   escreva("Digite a terceira nota:\n")
   leia(not3)
   escreva("Digite a quarta nota : \n")
   leia(not4)
   
  escreva(" Exelente!! a media é:",(not1+not2+not3+not4)/4,"\n")
  
  }
}
